programa
{
	
	funcao inicio()
	{
		inteiro n1,n2
		escreva("Digite o 1º número: ")
		leia(n1)
		escreva("Digite o 2º número: ")
		leia(n2)
		se(n1<=n2){
			para(n1;n1<=n2;n1++){
			escreva(n1,"\n")
			}
		}
	}
}
